<div class="contentGpCategoryCategory">
  <%- unless @docs.empty? -%>
  <section class="categoryType">
    <div class="docs">
      <ul>
        <%- @docs.each do |doc| -%>
        <li><%= doc_replace(doc, @piece.category_doc_style, @piece.date_style) %></li>
        <%- end -%>
      </ul>
    </div>
  </section>
  <%- end -%>

  <%= paginate @docs %>
</div>
