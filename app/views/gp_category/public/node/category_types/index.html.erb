<div class="contentGpCategoryCategoryTypes">
  <div class="category_types">
    <%- unless @category_types.empty? -%>
      <ul>
        <%- @category_types.each do |category_type| -%>
        <li><%= link_to category_type.title, category_type.public_uri %></li>
        <%- end -%>
      </ul>
    <%- end -%>
  </div>
  <%= paginate @category_types %>
</div>
