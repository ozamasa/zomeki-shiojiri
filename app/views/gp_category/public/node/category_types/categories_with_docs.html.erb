<div class="contentGpCategoryCategoryType">
  <%- @category_type.public_root_categories.each do |category| -%>
  <h2><%= link_to category.title, category.public_uri %></h2>
    <%- unless category.public_docs.empty? -%>
    <ul>
      <%- category.public_docs.each do |doc| -%>
      <li><%= render 'gp_category/public/shared/doc', doc: doc, doc_style: @content.category_type_doc_style, date_style: @content.date_style %></li>
      <%- end -%>
    </ul>
    <%- end -%>
  <%- end -%>
</div>
