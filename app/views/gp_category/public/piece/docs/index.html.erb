<%- unless @docs.empty? -%>
<ul class="docs">
  <%- @docs.each do |doc| -%>
  <li>
    <%- if doc.target.present? -%>
      <%- if doc.href.present? -%>
        <%= link_to doc.title, doc.href, target: doc.target %>
      <%- else -%>
        <%= doc.title -%>
      <%- end -%>
    <%- else -%>
      <%= link_to doc.title, doc.public_uri %>
    <%- end -%>
  </li>
  <%- end -%>
</ul>
<%- end -%>
