    <%- last_level_no = category.level_no -%>
  <%- else -%>
    <%- if last_level_no -%>
    <div class="categoryType categoryType<%= category.name.try(:camelize) %>">ãƒ»<%= link_to category.title, "#{category.category_type.public_uri}#{category.path_from_root_category}/" %></div>
    <%- last_level_no = category.level_no -%>
    <%- end -%>
  <%- end -%>
<%- end -%>
<%- end -%>
</div>
